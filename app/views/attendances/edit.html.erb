<%working_places = WorkingPlace.all%>
<%=form_with model: @attendance, local: true do |f|%>
  <%=f.label "出勤時刻"%>
  <%=f.time_field :attendance_time%>　<br>
  <%=f.label "退勤時刻"%>
  <%=f.time_field :leaving_time%><br>
  <%=f.label "勤務時間(分)"%>
  <%=f.number_field :working_times%><br>
  <%=f.label "早出(分)"%>
  <%=f.number_field :early_attendance%><br>
  <%=f.label "遅退(分)"%>
  <%=f.number_field :late_leaving%><br>
  <%=f.label "時間外労働(分)"%>
  <%=f.number_field :overtime_hours%><br>
  <%=f.label "休憩時間(分)"%>
  <%=f.number_field :rest_times%><br>
  <%=f.label "勤務場所"%>
  <%=f.collection_select :working_place,working_places,:address,:address%><br>
  <%= f.submit "編集"%>
<% end %>
